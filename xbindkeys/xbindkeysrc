# Tab controls
"xdotool keyup KP_Left key Control+Shift+Tab"
  KP_Left

"xdotool keyup KP_Begin key Control+w"
  KP_Begin

"xdotool keyup KP_Right key Control+Tab"
  KP_Right

# Music controls
"mpc --host htpc prev"
  KP_Home

"mpc --host htpc toggle"
  KP_Up

"mpc --host htpc next"
  KP_Prior

# Hotkeys
"termite"
  Mod4 + Return

"chromium"
  Mod4 + F

"chromium --incognito"
  Mod4 + Shift + F

"mangler"
  Mod4 + V

"subl3"
  Mod4 + W

"adb shell input keyevent 26"
  Mod4 + P

"sleep 0.5; xset dpms force off"
  Mod4 + BackSpace

# Room automation
"irsend -a htpc SEND_ONCE Monoprice_5557_HDMI_Switch 4"
  Mod4 + F12

"irsend -a htpc SEND_ONCE Monoprice_5557_HDMI_Switch 3"
  Mod4 + F11

"irsend -a htpc SEND_ONCE lg_mkj36998126 power"
  Mod4 + T
